{
    "name": "iet-ou/ou-embed",
    "description": "Wrapper round oEmbed-powered 'simexis/embed' library, with OU-specific goodness. | Â© 2017 The Open University.",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "IET_OU\\OU_Embed\\": "src/"
        }
    },
    "support": {
        "source": "https://github.com/IET-OU/ou-embed-php"
    },
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.3",
        "simexis/embed": "^1.4"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "squizlabs/php_codesniffer": "^3.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "scripts": {
        "test": "parallel-lint --exclude vendor . && phpcs -v --standard=PSR2 src/*.php"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "// Not using": {
            "essence/essence": "^3.4"
        }
    }
}
